page {
  background-color: #f5f5f5;
}
.feedbacktype {
  background-color: #fff;
  margin-top: 30rpx;
  padding: 0 25rpx;
  overflow: hidden;
}
.feedbacktype .text {
    font-size: 28rpx;
    margin-top: 35rpx;
}
.feedbacktype .ul {
    font-size: 0rpx;
}
.feedbacktype .ul .li {
      width: 140rpx;
      height: 55rpx;
      border: 1rpx solid #ff792a;
      color: #ff792a;
      text-align: center;
      font-size: 26rpx;
      display: inline-block;
      border-radius: 5px;
      overflow: hidden;
      margin: 25rpx 25rpx 25rpx 0;
      line-height: 55rpx;
      cursor: pointer;
}
.feedbacktype .ul .li.active {
        background-color: #ff792a;
        color: #fff;
}
.textarea {
  background-color: #fff;
  padding: 25rpx;
  margin-top: 25rpx;
}
.textarea textarea {
    width: 700rpx;
    height: 260rpx;
    font-size: 28rpx;
}
.register {
  width: 678rpx;
  height: 182rpx;
  position: relative;
  left: 50rpx;
  top: 480rpx;
  cursor: pointer;
}
.register image {
    width: 678rpx;
    height: 182rpx;
    display: block;
}

