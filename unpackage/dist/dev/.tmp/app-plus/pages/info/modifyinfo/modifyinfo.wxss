page {
  background-color: #f5f5f5;
}
.infocontent {
  background-color: #fff;
  padding: 25rpx;
  overflow: hidden;
  margin-top: 30rpx;
}
.infocontent .li {
    height: 100rpx;
    border-bottom: 1rpx solid #e2e2e2;
    line-height: 100rpx;
    position: relative;
    font-size: 28rpx;
}
.infocontent .li:last-child {
      border: 0;
}
.infocontent .li .int {
      width: 400rpx;
      height: 100rpx;
      text-align: right;
      position: absolute;
      right: 40rpx;
      top: 0;
}
.infocontent .li .portrait {
      width: 80rpx;
      height: 80rpx;
      position: absolute;
      right: 40rpx;
      top: 10rpx;
      border-radius: 50%;
      overflow: hidden;
}
.infocontent .li .arrow {
      width: 14rpx;
      height: 26rpx;
      position: absolute;
      right: 0;
      top: 38rpx;
}
.register {
  width: 678rpx;
  height: 182rpx;
  position: relative;
  left: 50rpx;
  top: 620rpx;
  cursor: pointer;
}
.register image {
    width: 678rpx;
    height: 182rpx;
    display: block;
}

