.Top_bar {
  background-color: #fff;
  border-bottom: 1px solid #e2e2e2;
  color: #333;
}
.header {
  width: 100%;
  height: 91rpx;
  line-height: 90rpx;
  text-align: center;
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
}
.header .img {
    width: 100rpx;
    height: 90rpx;
}
.header .img image {
      width: 19rpx;
      height: 35rpx;
      display: block;
      position: absolute;
      left: 25rpx;
      top: 50%;
      margin-top: -17.5rpx;
}
.header .text {
    width: 400rpx;
    height: 90rpx;
    line-height: 90rpx;
    position: absolute;
    left: 50%;
    top: 0;
    margin-left: -200rpx;
    text-align: center;
    font-size: 36rpx;
}

