page {
  background-color: #f5f5f5;
}
.year {
  width: 750rpx;
  overflow: hidden;
  background-color: #fff;
  margin: 30rpx auto;
}
.year .li {
    width: 690rpx;
    margin: 0 auto;
    border-bottom: 1rpx solid #e2e2e2;
    height: 100rpx;
    line-height: 100rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}
.year .li .img {
      display: inline-block;
      width: 80rpx;
      height: 50rpx;
      margin: 12.5rpx 30rpx 0 0;
}
.year .li .img image {
        width: 50rpx;
        height: 50rpx;
}
.year .li .name {
      width: 220rpx;
      display: inline-block;
}
.year .li .price {
      width: 380rpx;
      text-align: right;
      display: inline-block;
      color: #ff792a;
}
.year .li:last-child {
    border: 0;
}
.equity {
  padding: 30rpx 20rpx 0 20rpx;
  border-top: 1rpx solid #e2e2e2;
  font-size: 28rpx;
  height: 680rpx;
  overflow: hidden;
  background-color: #fff;
}
.equity scroll-view {
    width: 100%;
    height: 100%;
}
.equity .txt {
    margin-top: 20rpx;
}
.register {
  width: 678rpx;
  height: 182rpx;
  position: absolute;
  left: 50rpx;
  bottom: -30rpx;
  cursor: pointer;
}
.register image {
    width: 678rpx;
    height: 182rpx;
    display: block;
}

