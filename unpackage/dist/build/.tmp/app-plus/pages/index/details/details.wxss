.layerFixed {
  width: 100%;
  height: 100%;
  background-color: red;
  position: fixed;
  z-index: 3;
  left: 0;
  height: 100%;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
}
.layerFixed .contract {
    width: 690rpx;
    height: 1152rpx;
    background-color: #fff;
    margin: 64rpx auto;
    border-radius: 10rpx;
    overflow: hidden;
}
.layerFixed .contract .htSuer {
      width: 260rpx;
      height: 60rpx;
      margin: 20rpx auto 0;
      background-color: #ff792a;
      border-radius: 10rpx;
      text-align: center;
      line-height: 60rpx;
      color: #fff;
      font-size: 30rpx;
}
.layerFixed .contract scroll-view {
      width: 630rpx;
      height: 1052rpx;
      margin: 0 auto;
}
.layerFixed .ListSelect {
    padding: 25rpx 0 0 0;
    background-color: #fff;
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    font-size: 28rpx;
}
.layerFixed .ListSelect .bigTit {
      height: 55rpx;
      line-height: 55rpx;
      font-weight: bold;
      padding: 0 25rpx;
}
.layerFixed .ListSelect .timelist {
      overflow: hidden;
      line-height: 55rpx;
      padding: 0 25rpx;
}
.layerFixed .ListSelect .timelist .li {
        width: 140rpx;
        display: inline-block;
        color: #ff792a;
        border: 1rpx solid #ff792a;
        border-radius: 5px;
        overflow: hidden;
        text-align: center;
        line-height: 55rpx;
        display: inline-block;
        margin-right: 25rpx;
        margin: 25rpx 25rpx 0 0;
}
.layerFixed .ListSelect .timelist .li.active {
          background-color: #ff792a;
          color: #fff;
}
.layerFixed .ListSelect .smallTit {
      height: 55rpx;
      line-height: 55rpx;
      padding: 0 25rpx;
}
.layerFixed .ListSelect .smallTit:after {
        content: '*';
        color: #ff792a;
        line-height: 55rpx;
}
.layerFixed .ListSelect .picker {
      width: 700rpx;
      margin: 10rpx auto;
      height: 70rpx;
      border: 1rpx solid #E2E2E2;
